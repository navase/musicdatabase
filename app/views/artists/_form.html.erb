<%= form_for(@artist) do |f| %>

  <p><%= f.label :name %></p>
  <p><%= f.text_field :name %></p>

  <p><%= f.label :biography %></p>
  <p><%= f.text_field :biography %></p>

  <p><%= f.label :genre %></p>
  <p><%= f.text_field :genre %></p>

  <p><%= f.label :image %></p>
  <p><%= f.file_field :image %></p>

  <p><%= f.submit "Save" %></p>

<% end %>
